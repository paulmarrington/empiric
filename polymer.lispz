(var polymer-cdn "http://www.polymer-project.org/1.0/samples/components/")
(var polymer-init! (lambda ()
  (dom-append! "head"
    (dom-element script src (+ polymer-cdn "webcomponentsjs/webcomponents-lite.js"))
    (dom-element link rel "import" href (+ polymer-cdn "polymer/polymer.html"))
)))

(var polymer-proto-element! (lambda ()
  (Polymer is "proto-element" ready (lambda
    (set! this.textContent "I'm a proto-element. Check out my prototype!")))
))